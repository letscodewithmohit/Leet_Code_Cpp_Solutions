class Solution {
public:
    bool canBeEqual(vector<int>& target, vector<int>& arr) {
        sort(arr.begin(),arr.end());
          sort(target.begin(),target.end());
        int n = target.size();
        bool f = 1;
        for(int i=0; i<n; i++){
            if(arr[i]==target[i]){
             continue;
            }
            else{
                f = 0;
            }
        }
    
    return f;
    }
};
