class Solution {
public:
    bool checkIfExist(vector<int>& arr) {
        for(int i=0; i<arr.size(); i++){
            for(int j=0; j<arr.size(); j++){
             if(i != j && i >= 0 && j <arr.size()){
                if(arr[i] == 2*arr[j]){
                    return true;
                    break;
                }
        }
        return false;
    }
};
